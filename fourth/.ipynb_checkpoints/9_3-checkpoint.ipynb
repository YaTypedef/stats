    "# encoding: utf-8\n",
    "%matplotlib inline\n",
    "\n",
    "import numpy as np\n",
    "from scipy.stats import norm\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N = 100\n",
    "\n",
    "def generate():\n",
    "    return norm.rvs(size=N)\n",
    "\n",
    "def draw():\n",
    "    x = np.linspace(-5, 5, 100)\n",
    "    plt.plot(x, norm.pdf(x))\n",
    "    plt.show()\n",
    "\n",
    "draw()"
